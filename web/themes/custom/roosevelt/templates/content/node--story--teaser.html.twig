{#
/**
 * @file
 * Theme override to display a node.
 *
 */
#}

<div class="internal-storyListing-Container">
  <div class="internal-storyListing-contentWrapper">
    {{ title_prefix }}
    <h4>
      <a class="internal-storyListing-contentTitleLink" href="{{ url }}">{{ label }}</a>
    </h4>
    {{ title_suffix }}
    <div class="internal-storyListing-contentPersonName">{{ author_name }}</div>
    <div class="internal-storyListing-contentAttribute"></div>
    <div class="internal-storyListing-contentTeaser"><p>{{ content.body|render|striptags|trim() }}</p></div>
    <div class="internal-storyListing-taxButtonContainer">{{ tag_list }}</div>
  </div>
  <div class="internal-storyListing-imgWrapper">
    {{ content.field_featured_image }}
  </div>
</div>