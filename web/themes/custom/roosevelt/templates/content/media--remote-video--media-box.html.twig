{#
/**
 * @file
 * Theme override to display a media item.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}
{% if content %}
  {% include "@organisms/textsection/_mediabox.twig" with {
     "content": content.field_media_oembed_video,
     "variant": media_variant
    }
  %}
{% endif %}
