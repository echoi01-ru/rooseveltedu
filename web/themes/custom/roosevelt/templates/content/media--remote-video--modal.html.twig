{#
/**
 * @file
 * Theme override to display a media item.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}
{% if content %}
  {% include "@organisms/textsection/_section-cta.twig" with {
      "modal_url": modal_url,
      "title": content.field_link_label|field_value,
      "variant": variant|clean_class,
    }
  %}
{% endif %}
