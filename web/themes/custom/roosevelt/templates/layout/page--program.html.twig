{#
/**
 * @file
 * Theme override to display a single page.
 *
 * @see template_preprocess_page()
 * @see html.html.twig
 */
#}
<div class="internalNavigationController">
  {{ page.announcement }}
  {{ page.header }}
</div>{# /.internalNavigationController #}

<a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

{% if page.content_title %}
<div class="internal-upperContainer" role="banner">
  <div id="plcheader_0_pnlTitleContainer" class="internal-upperContainer-titleContainer">
    <div class="globalWidth">
      {{ page.content_title }}
    </div>
  </div>
  <div class="internal-upperContainer-overlay"></div>
</div>
{% endif %}

{{ page.help }}
{{ page.message }}

{{ page.admin_tasks }}

<div class="internal-contentWrapper">
  <div class="internal-contentInnerWrapper programDetails">
    <div class="globalWidth">
      {{ page.content }}
    </div>
  </div>
</div>

{% if page.footer_first or page.footer_second or page.footer_lower or page.footer_bottom or page.footer %}
<footer class="footer-container" role="contentinfo">
  <div class="globalWidth footer-innerContainer">

    {% if page.footer_first or page.footer_second %}
    <div class="footer-gridContainer">
      {{ page.footer_first }}
      {{ page.footer_second }}
    </div>
    {% endif %}

    {{ page.footer_lower }}
    {{ page.footer_bottom }}
    {{ page.footer }}
  </div>
</footer>
{% endif %}

<div id="landing-slider"></div>
